provkab == 1302 ~ 2,
provkab == 1303 ~ 3,
provkab == 1304 ~ 4,
provkab == 1305 ~ 5,
provkab == 1306 ~ 6,
provkab == 1307 ~ 7,
provkab == 1308 ~ 8,
provkab == 1309 ~ 9,
provkab == 1310 ~ 10,
provkab == 1311 ~ 11,
provkab == 1312 ~ 12,
