datasumbar <- datasumbar |>
mutate(jk =
ifelse(B4K4 == 1, 0, 1)) |>
set_value_labels(jk = c("Laki-laki" = 0, Perempuan = 1)) |>
set_variable_labels(jk = "Jenis Kelamin")
# recode variabel usia
datasumbar <- datasumbar |>
mutate(usia = case_when(
